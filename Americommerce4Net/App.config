<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="ServiceURL" value="https://--yourstore--/api/v1" />
    <add key="ApiAccessToken" value="--your api access token--" />
    <add key="RequestTimeout" value="20000" />
    <add key="ErrorRetryMax" value="5" />
    <add key="ErrorRetryDelay" value="60000" />
    <add key="ThrottlingEnabled" value="true" />
    <add key="ThrottlingDelay" value="11000" />
    <add key="AllowDeletions" value="false" />
    <add key="EnableLogging" value="true" />
  </appSettings>
</configuration>